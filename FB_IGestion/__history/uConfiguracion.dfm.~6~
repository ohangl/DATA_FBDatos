  object dsTerminales: TDataSource
    AutoEdit = False
    DataSet = Terminales
    OnDataChange = dsTerminalesDataChange
    Left = 114
    Top = 310
  end
  object qTerminales: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trGestion
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select IDPUNTOVENTA, IDSUCURSAL, SECTOR, FANTASY1,'
      '             FANTASY2, DIRECCION, LOCALIDAD, PROVINCIA,'
      '             CODPOSTAL, TELEFONO, MAILWEB, PIECOMPROBANTE,'
      '             CENTAVOS, CENTAVOSPRECIOS, CONTROLSTOCK,'
      '             CONTROLADOR_IMPRESORA, IMPRIMEENCONTROLADOR,'
      '             CAJONDINERO, CIERREAUTOMATICO, SERVIDORFISCAL,'
      '             TIEMPOFISCAL, HORACIERRE, IMPRIMEENLINEA,'
      '             CONTROLADORLOCAL, PUERTOCONTROLADOR,'
      '             VELOCIDADCONTROLADOR, MARCACONTROLADOR,'
      '             MODELOCONTROLADOR, TIPOCONTROLADOR,'
      '             CONTROLADORCONLOGO, NROPTOVTACONTROLADOR,'
      '             AVISARSTOCKMINIMO, PEDIRVENDEDOR,'
      
        '             PEDIRDESCUENTO, PEDIRLISTAPRECIO,                  ' +
        '   '
      '             LISTAPRECIOSOFERTAS, HACERNOTACREDITO,'
      '             CONTROL_TALLES, MOSTRARSALDOSCC, PEDIRSUBDET,'
      '             HACERREMITOS, CONTROLREMITOS, NFCTACTE,'
      '             NFTARJETA, NFCHEQUE, NFPENDIENTE, NFCREDITO,'
      '             COMPA, NOTCREDA, COMPB, NOTCREDB, COMPT, RECX,'
      '             REMX, REMITOT, PRESUPUESTO, NOTAPEDIDO, DNOFS,'
      '             DOCINTERNOS, INVERTIRCANTIDAD, AVISOSMORA,'
      '             MODIFICARIMPERSONAL, CANTIDADPRODUCTOS,'
      '             ESTADO, UENTRADA, USALIDA, USUARIO,'
      '             IMPRIMIRMARCO, IMPRIMIRTRIPLICADO,'
      '             CONTROLCOMPROBANTES, PTOISSERVER, ACUERDOEXJ,'
      '             MODIMPRESORA, ULTIMOSPRECIOS, IDEMPRESA'
      ' From CFGPTOVTA'
      ' Where (IDEMPRESA = :E) And'
      '            (IDSUCURSAL = :S) And'
      '            (IDPUNTOVENTA = :P)'
      ' Order By IDEMPRESA, IDSUCURSAL, IDPUNTOVENTA')
    Left = 84
    Top = 310
    ParamData = <
      item
        DataType = ftInteger
        Name = 'E'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'S'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'P'
        ParamType = ptUnknown
      end>
  end
  object prTerminales: TDataSetProvider
    DataSet = qTerminales
    Options = [poIncFieldProps]
    Left = 24
    Top = 310
  end
