  object qViewEmp: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trProc
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select E.IDEMPLEADO, E.NOMBRE, E.TELEFONOS, '
      '          E.DIRECCION, E.ACTIVO, E.IdSucAsignada, '
      '          F.FUNCION,  K.DETSUC'
      'From EMPLEADOS E'
      'Left Outer Join FUNCTIONS F'
      '  on F.IDFUNCION = E.IDTAREAFUNCION'
      'Left Outer Join Sucursales K'
      '  on K.IdSucursal = E.IdSucAsignada'
      'Order By E.NOMBRE')
    Left = 202
    Top = 222
    object qViewEmpIDEMPLEADO: TIntegerField
      FieldName = 'IDEMPLEADO'
