  object qAjustes: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trProc
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      
        'Select I.IdFactura, I.IdSucursal, I.IdPuntoVenta, I.IdSucursalD,' +
        ' '
      
        '           I.FechaF, I.IdArticulo, I.TMov, I.Cantidad, I.TipoAj,' +
        ' A.Marca, '
      '           A.Detalle, S.SubGrupo, K.DetSuc, E.Nombre'
      'From ItemsTR I'
      'Join Articulos A'
      '  on A.IdArticulo = I.IdArticulo'
      'Join SubGrupos S'
      '  on S.IdGrupo = A.IdGrupo And '
      '       S.IdSubGrupo = A.IdSubGrupo'
      'Join Sucursales K'
      '  on K.IdSucursal = I.IdSucursalD'
      'Join Traspaso T'
      '  on T.IdFactura = I.IdFactura and '
      '       T.IdSucursal = I.IdSucursal and '
      '       T.IdPuntoVenta = I.IdPuntoVenta'
      'Join Empleados E'
      '  on E.IdEmpleado = T.Empleado'
      'Where (I.TMov in (46,52)) and '
      '            (I.FechaF between :Desde and :Hasta)'
      'Order By A.Marca, A.Detalle'
      ''
      ' ')
    Left = 23
    Top = 163
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
    object qAjustesSubGrupo: TStringField
      DisplayWidth = 60
      FieldName = 'SubGrupo'
      Origin = '"SUBGRUPOS"."SUBGRUPO"'
      Size = 60
    end
    object qAjustesDetSuc: TStringField
      DisplayWidth = 2
      FieldName = 'DetSuc'
      Origin = '"SUCURSALES"."DETSUC"'
      Size = 2
    end
    object qAjustesFechaF: TDateField
      DisplayWidth = 10
      FieldName = 'FechaF'
      Origin = '"ITEMSTR"."FECHAF"'
      DisplayFormat = 'dd/mm/yy'
    end
    object qAjustesIdArticulo: TIntegerField
      DisplayWidth = 10
      FieldName = 'IdArticulo'
      Origin = '"ITEMSTR"."IDARTICULO"'
    end
    object qAjustesMarca: TStringField
      DisplayWidth = 20
      FieldName = 'Marca'
      Origin = '"ARTICULOS"."MARCA"'
    end
    object qAjustesDetalle: TStringField
      DisplayWidth = 60
      FieldName = 'Detalle'
