      'WHERE'
      '  IDCOMPROBANTE = :OLD_IDCOMPROBANTE')
    RefreshSQL.Strings = (
      'SELECT '
      '  IDCOMPROBANTE,'
      '  DESCRIPCION,'
      '  DESCCORTA,'
      '  IP1,'
      '  IP2,'
      '  IP3,'
      '  IP4,'
      '  IP5,'
      '  IP6,'
      '  IP7,'
      '  IP8,'
      '  IP9,'
      '  IP10,'
      '  TC_SIAP'
      'FROM TIPOSCOMP '
      'WHERE'
      '  IDCOMPROBANTE = :IDCOMPROBANTE')
    SelectSQL.Strings = (
      'Select IDCOMPROBANTE, DESCRIPCION, DESCCORTA, '
      '          IP1, IP2, IP3, IP4, IP5, IP6, IP7, IP8, IP9, IP10, '
