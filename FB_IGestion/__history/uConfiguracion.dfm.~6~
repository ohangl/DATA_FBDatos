  object qModPrc: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trProc
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      
        'Select P.IdArticulo, P.TipoComp, P.NroRemito, P.DiaHora, P.Usuar' +
        'io,'
      
        '           P.PrecioOld, P.PrecioNew, A.Marca, A.Detalle, S.SubGr' +
        'upo, '
      '           E.Nombre,T.DescCorta'
      'From ArtModPrc P'
      'Join Articulos A'
      '  on A.IdArticulo = P.IdArticulo'
      'Join SubGrupos S'
      '  on S.IdGrupo = A.IdGrupo And '
      '       S.IdSubGrupo = A.IdSubGrupo'
      'Join Empleados E'
      '  on E.IdEmpleado = P.Usuario'
      'Join TiposComp T'
      '  on T.IdComprobante = P.TipoComp'
      'Where P.DiaHora between :Desde and :Hasta'
      'Order By A.Marca, A.Detalle, P.DiaHora '
      '')
    Left = 114
    Top = 281
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
    object qModPrcSubGrupo: TStringField
      DisplayWidth = 60
      FieldName = 'SubGrupo'
      Origin = '"SUBGRUPOS"."SUBGRUPO"'
      Size = 60
    end
    object qModPrcDiaHora: TDateTimeField
      DisplayWidth = 18
      FieldName = 'DiaHora'
      Origin = '"ARTMODPRC"."DIAHORA"'
    end
    object qModPrcDescCorta: TStringField
      DisplayWidth = 5
      FieldName = 'DescCorta'
      Origin = '"TIPOSCOMP"."DESCCORTA"'
      Size = 5
    end
    object qModPrcNroRemito: TStringField
      DisplayWidth = 13
      FieldName = 'NroRemito'
      Origin = '"ARTMODPRC"."NROREMITO"'
      Size = 13
    end
    object qModPrcIdArticulo: TIntegerField
      DisplayWidth = 10
      FieldName = 'IdArticulo'
      Origin = '"ARTMODPRC"."IDARTICULO"'
    end
    object qModPrcMarca: TStringField
      FieldName = 'Marca'
      Origin = '"ARTICULOS"."MARCA"'
    end
    object qModPrcDetalle: TStringField
      FieldName = 'Detalle'
      Origin = '"ARTICULOS"."DETALLE"'
      Size = 60
    end
    object qModPrcNombre: TStringField
      DisplayWidth = 30
      FieldName = 'Nombre'
      Origin = '"EMPLEADOS"."NOMBRE"'
      Size = 30
    end
    object qModPrcTipoComp: TSmallintField
      DisplayWidth = 10
      FieldName = 'TipoComp'
      Origin = '"ARTMODPRC"."TIPOCOMP"'
      Visible = False
    end
    object qModPrcUsuario: TIntegerField
      DisplayWidth = 10
      FieldName = 'Usuario'
      Origin = '"ARTMODPRC"."USUARIO"'
      Visible = False
    end
    object qModPrcPRECIOOLD: TMDOBCDField
      FieldName = 'PRECIOOLD'
      Origin = '"ARTMODPRC"."PRECIOOLD"'
      currency = True
      Precision = 18
      Size = 4
    end
    object qModPrcPRECIONEW: TMDOBCDField
      FieldName = 'PRECIONEW'
      Origin = '"ARTMODPRC"."PRECIONEW"'
      currency = True
      Precision = 18
      Size = 4
    end
  end
