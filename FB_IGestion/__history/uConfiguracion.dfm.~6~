    LoadDefaults = False
    SQL.Strings = (
      'Select IdArticulo, IdStock, IdFactura, IdSucursal,'
      '           FechaF,  PrecioFinal, Cantidad, Marca, Detalle, '
      '           DetSuc, NroFactura'
      'From PRODUCTOS_VENDIDOS_SINSTOCK(:Desde, :Hasta)')
    Left = 234
    Top = 281
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
