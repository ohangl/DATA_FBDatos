  object qEstadosTerm: TMDOQuery
    AfterOpen = qEstadosTermAfterOpen
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trGestion
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select T.IdSucursal, T.IdPuntoVenta, T.Estado, '
      '           T.UEntrada, T.USalida, T.Usuario, E.Nombre'
      'From CFGPTOVTA T'
      'Left Outer Join Empleados E'
      '  on E.IdEmpleado = T.Usuario'
      'Order By T.IdSucursal, T.IdPuntoVenta')
    Left = 24
    Top = 398
