  object qTiposComp: TMDODataSet
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trGestion
    BufferChunks = 1000
    CachedUpdates = False
    DeleteSQL.Strings = (
      'DELETE FROM TIPOSCOMP'
      'WHERE'
      '  IDCOMPROBANTE = :OLD_IDCOMPROBANTE')
    InsertSQL.Strings = (
      'INSERT INTO TIPOSCOMP'
      
        '  (IDCOMPROBANTE, DESCRIPCION, DESCCORTA, IP1, IP2, IP3, IP4, IP' +
        '5, IP6, '
      '   IP7, IP8, IP9, IP10, TC_SIAP)'
      'VALUES'
      
        '  (:IDCOMPROBANTE, :DESCRIPCION, :DESCCORTA, :IP1, :IP2, :IP3, :' +
        'IP4, :IP5, '
      '   :IP6, :IP7, :IP8, :IP9, :IP10, :TC_SIAP)')
    LoadDefaults = False
