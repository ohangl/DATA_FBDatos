      Origin = '"PRODUCTOS_VENDIDOS_CAJA"."IDARTICULO"'
    end
    object qVentasFECHAF: TDateField
      FieldName = 'FECHAF'
      Origin = '"PRODUCTOS_VENDIDOS_CAJA"."FECHAF"'
    end
    object qVentasCANTIDAD: TIntegerField
