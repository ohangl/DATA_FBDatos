  object qBajoMarkUp: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trProc
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      
        'Select A.IdArticulo, A.IdGrupo, A.IdSubGrupo, A.Marca, A.Detalle' +
        ', '
      
        '           A.Costo, A.ImpInt, A.FechaAlta, A.IdMarkUp, A.Gananci' +
        'a, '
      '           A.Precio, M.CoefMarkUp, S.SubGrupo'
      'From Articulos A'
      'Join MarkUp M'
      '  on M.IdMarkUp = A.IdMarkUp'
      'Join SubGrupos S'
      '  on S.IdGrupo = A.IdGrupo and S.IdSubGrupo = A.IdSubGrupo'
      'Where ((A.Ganancia-1) < M.CoefMarkUp) And'
      '            (A.FechaAlta between :Desde And :Hasta)'
      'Order By A.Marca, A.Detalle')
    Left = 83
    Top = 193
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
    object qBajoMarkUpSubGrupo: TStringField
      FieldName = 'SubGrupo'
      Origin = '"SUBGRUPOS"."SUBGRUPO"'
      Size = 60
    end
    object qBajoMarkUpFechaAlta: TDateField
      FieldName = 'FechaAlta'
      Origin = '"ARTICULOS"."FECHAALTA"'
      DisplayFormat = 'dd/mm/yy'
    end
    object qBajoMarkUpIdArticulo: TIntegerField
      FieldName = 'IdArticulo'
      Origin = '"ARTICULOS"."IDARTICULO"'
    end
    object qBajoMarkUpMarca: TStringField
      FieldName = 'Marca'
      Origin = '"ARTICULOS"."MARCA"'
    end
    object qBajoMarkUpDetalle: TStringField
      FieldName = 'Detalle'
      Origin = '"ARTICULOS"."DETALLE"'
      Size = 60
    end
    object qBajoMarkUpIdGrupo: TIntegerField
      FieldName = 'IdGrupo'
      Origin = '"ARTICULOS"."IDGRUPO"'
      Visible = False
    end
    object qBajoMarkUpIdSubGrupo: TIntegerField
      FieldName = 'IdSubGrupo'
      Origin = '"ARTICULOS"."IDSUBGRUPO"'
      Visible = False
    end
    object qBajoMarkUpIdMarkUp: TIntegerField
      FieldName = 'IdMarkUp'
      Origin = '"ARTICULOS"."IDMARKUP"'
      Visible = False
    end
    object qBajoMarkUpCOSTO: TMDOBCDField
      FieldName = 'COSTO'
      Origin = '"ARTICULOS"."COSTO"'
      currency = True
      Precision = 18
      Size = 4
    end
    object qBajoMarkUpIMPINT: TMDOBCDField
      FieldName = 'IMPINT'
      Origin = '"ARTICULOS"."IMPINT"'
      currency = True
      Precision = 18
      Size = 4
    end
    object qBajoMarkUpGANANCIA: TMDOBCDField
      FieldName = 'GANANCIA'
      Origin = '"ARTICULOS"."GANANCIA"'
      DisplayFormat = '0.000'
      Precision = 18
      Size = 4
    end
    object qBajoMarkUpPRECIO: TMDOBCDField
      FieldName = 'PRECIO'
      Origin = '"ARTICULOS"."PRECIO"'
      currency = True
      Precision = 18
      Size = 4
    end
    object qBajoMarkUpCOEFMARKUP: TMDOBCDField
      FieldName = 'COEFMARKUP'
      Origin = '"MARKUP"."COEFMARKUP"'
      DisplayFormat = '0.000'
      Precision = 18
      Size = 4
    end
  end
  object qOrdenes: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trProc
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select O.IDORDEN, O.FECHAOP, O.NROCOMPOP, O.TOTALOP, '
      '           O.CONTADO, O.TICKETS, O.BANCOPROP, O.BANCOTER, '
      '           O.BONIFPROV, O.RETENCIONES, O.USUARIO, P.Nombre'
      'From ORDENES O'
      'Join Proveedores P'
      '   on P.IdProveedor = O.Entidad'
      'Where (O.Estado > 0) And '
      '            (O.FechaOp between :Desde And :Hasta)')
    Left = 203
    Top = 193
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
