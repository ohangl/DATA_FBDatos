  object qCaja: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select F.IdFactura, F.IdSucursal, F.IdPuntoVenta, F.IdCompRef, '
      
        '           F.TipoFactura, F.DiaHora, F.FechaF, F.NroFactura, F.C' +
        'liente, '
      
        '           F.Total, F.Contado, F.Tarjeta, F.CtaCte, F.IdSubDep, ' +
        'F.NroCtas, '
      
        '           F.Cheque, F.Tickets, F.Bonificacion, F.OtrosPagos, F.' +
        'CobSenas, '
      
        '           F.Empleado, F.CantArtic, C.Nombre as Entidad, S.DetSu' +
        'c,'
      '           E.Nombre as Vendedor, T.DescCorta as TipoFac, '
      '           T.Descripcion as TipoFacLarga '
      'From FacVen F'
      'Join Clientes C'
      '  on C.IdCliente = F.Cliente'
      'Join Empleados E'
      '  on E.IdEmpleado = F.Empleado'
      'Join TiposComp T'
      '  on T.IdComprobante = F.TipoFactura'
      'Join Sucursales S'
      '  on S.IdSucursal = F.IdSucursal'
      'Where F.FechaF between :Desde and :Hasta'
      'Order By F.IdSucursal, F.FechaF, F.TipoFactura, F.NroFactura'
      ' '
      ' ')
    Left = 21
    Top = 480
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
