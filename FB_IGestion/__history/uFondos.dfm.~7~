  object qSucur: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select F.IdSucursal, Sum(F.Total) as Total, S.NombreSuc, '
      '         Count(F.IdFactura) as CantOp'
      'From FacVen F'
      'Join Sucursales S'
      '  on S.IdSucursal = F.IdSucursal'
      'Where FechaF between :Desde And :Hasta'
      'Group By F.IdSucursal, S.NombreSuc '
      'Order By F.IdSucursal')
    Left = 138
    Top = 480
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
