  object qMoneyC: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select Sum(Contado) as Efectivo, Sum(Tarjeta) as Tarjeta, '
      
        '          Sum(Bonificacion)  as Bonificacion, Sum(OtrosPagos) as' +
        ' Otros,'
      '          Sum(Tickets) as Tickets, Sum(Cheque) as Cheques, '
      '          Sum(CobSenas) as Senas'
      'From FacVen'
      'Where FechaF between :Desde And :Hasta')
    Left = 223
    Top = 452
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
