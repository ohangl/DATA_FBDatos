  object qVentaS: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select F.FechaF, F.IdSucursal, S.NombreSuc as Sucursal,'
      
        '       Sum(F.IvaAlicuota1) As IvaVta, Sum(F.NoComputables) as Im' +
        'pInt, '
      '       Sum(F.Total) as Total, Sum(F.Total) as GTotal    '
      'From FacVen  F'
      'Left Outer Join Sucursales S'
      '  on S.IdSucursal = F.IdSucursal'
      'Where F.FechaF between :Desde and :Hasta '
      '           And F.TipoFactura in (1,2,3,5,6,7,8) '
      '           And F.State = 0'
      'Group By F.FechaF, F.IdSucursal, S.NombreSuc'
      'Order By F.FechaF, F.IdSucursal'
      ''
      ' '
      ' ')
    Left = 80
    Top = 452
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
    object qVentaSFechaF: TDateField
      FieldName = 'FechaF'
    end
    object qVentaSIdSucursal: TIntegerField
      FieldName = 'IdSucursal'
    end
    object qVentaSSucursal: TStringField
      FieldName = 'Sucursal'
    end
    object qVentaSIVAVTA: TMDOBCDField
      FieldName = 'IVAVTA'
      currency = True
      Precision = 18
      Size = 4
    end
    object qVentaSIMPINT: TMDOBCDField
      FieldName = 'IMPINT'
      currency = True
      Precision = 18
      Size = 4
    end
    object qVentaSTOTAL: TMDOBCDField
      FieldName = 'TOTAL'
      currency = True
      Precision = 18
      Size = 4
    end
    object qVentaSGTOTAL: TMDOBCDField
      FieldName = 'GTOTAL'
      currency = True
      Precision = 18
      Size = 4
    end
  end
  object qTVentaS: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select F.IdSucursal, '
      '           S.NombreSuc as Sucursal, '
      '           Sum(F.IvaAlicuota1) As TIvaVta, '
      '           Sum(F.NoComputables) as TImpInt, '
      '           Sum(F.Total) as TTotal'
      'From FacVen F'
      'Left Outer Join Sucursales S'
      '  on S.IdSucursal = F.IdSucursal'
      'Where F.FechaF between :Desde and :Hasta '
      '            And F.TipoFactura in (1,2,3,5,6,7,8) '
      '            And F.State = 0'
      'Group By F.IdSucursal, S.NombreSuc'
      'Order By F.IdSucursal')
    Left = 109
    Top = 452
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end>
    object qTVentaSIdSucursal: TIntegerField
      FieldName = 'IdSucursal'
    end
    object qTVentaSSucursal: TStringField
      FieldName = 'Sucursal'
    end
    object qTVentaSTIVAVTA: TMDOBCDField
      FieldName = 'TIVAVTA'
      currency = True
      Precision = 18
      Size = 4
    end
    object qTVentaSTIMPINT: TMDOBCDField
      FieldName = 'TIMPINT'
      currency = True
      Precision = 18
      Size = 4
    end
    object qTVentaSTTOTAL: TMDOBCDField
      FieldName = 'TTOTAL'
      currency = True
      Precision = 18
      Size = 4
    end
  end
