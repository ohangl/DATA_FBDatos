  object qArtVend: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select IdArticulo, IdFactura, IdSucursal, IdPuntoVenta,'
      '           FechaF, Cantidad, PrecioTotal, Oferta, IdArea, '
      '          IdGrupo, IdSubGrupo, DetArt, DetGrp, DetSGr,DetSuc'
      'From PRODUCTOS_VENDIDOS_SUCURSAL(:Desde, :Hasta, :IdSuc)')
    Left = 500
    Top = 154
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'IdSuc'
        ParamType = ptUnknown
      end>
