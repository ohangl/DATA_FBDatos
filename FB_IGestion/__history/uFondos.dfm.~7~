  object qAreas: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = trCaja
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      'Select R.Area, S.DetSuc, G.Grupo, '
      '          Sum(I.PrecioTotal) as Total'
      'From FacVen F '
      'Join ItemsFV I'
      '  on (I.IdFactura = F.IdFactura) And '
      '       (I.IdSucursal = F.IdSucursal) And '
      '       (I.IdPuntoVenta = F.IdPuntoVenta) '
      'Join Articulos A'
      '  on A.IdArticulo = I.IdArticulo'
      'Join Areas R'
      '  on R.IdArea = A.IdArea'
      'Join Grupos G'
      '  on (G.IdArea = A.IdArea) and '
      '       (G.IdGrupo = A.IdGrupo)'
      'Join Sucursales S'
      '  on S.IdSucursal = F.IdSucursal'
      'Where F.State = 0 and'
      '            F.TipoFactura in (1,2,3,5,6,7,8,90) And'
      '            F.FechaF between :Desde And :Hasta'
      'Group By R.Area, S.DetSuc, G.Grupo'
      'Order By R.Area, S.DetSuc, G.Grupo')
    Left = 428
    Top = 480
    ParamData = <
      item
        DataType = ftDate
        Name = 'Desde'
        ParamType = ptUnknown
        Value = 39203d
      end
      item
        DataType = ftDate
        Name = 'Hasta'
        ParamType = ptUnknown
        Value = 39233d
      end>
    object qAreasAREA: TMDOStringField
      FieldName = 'AREA'
      Origin = '"AREAS"."AREA"'
      Required = True
      Size = 40
    end
    object qAreasDETSUC: TMDOStringField
      FieldName = 'DETSUC'
      Origin = '"SUCURSALES"."DETSUC"'
      Size = 2
    end
    object qAreasGRUPO: TMDOStringField
      FieldName = 'GRUPO'
      Origin = '"GRUPOS"."GRUPO"'
      Required = True
      Size = 60
    end
    object qAreasTOTAL: TMDOBCDField
      FieldName = 'TOTAL'
      currency = True
      Precision = 18
      Size = 4
    end
  end
