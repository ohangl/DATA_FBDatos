  object qVendidos: TMDOQuery
    Database = dmGestion.dbGestion
    Transaction = dmGestion.trGestion
    BufferChunks = 1000
    CachedUpdates = False
    LoadDefaults = False
    SQL.Strings = (
      
        'Select I.IdFactura, I.IdSucursal, I.IdPuntoVenta, I.IdArticulo, ' +
        'I.Oferta,'
      
        '           I.Talle, A.Marca, A.Detalle, R.Area, G.Grupo, S.SubGr' +
        'upo,'
      
        '           K.Color, I.Cantidad, I.PrecioTotal, F.NroFactura, F.F' +
        'echaF, '
      '           H.DetSuc, C.Nombre'
      'From FacVen F'
      'Join ItemsFV I'
      
        '  on (I.IdFactura = F.IdFactura And I.IdSucursal = F.IdSucursal ' +
        'and I.IdPuntoVenta = F.IdPuntoVenta)'
      'Join Articulos A'
      '  on A.IdArticulo = I.IdArticulo'
      'Join Areas R'
      '  on R.IdArea = A.IdArea'
      'Join Grupos G'
      '  on (G.IdArea = A.IdArea And G.IdGrupo = A.IdGrupo)'
      'Join SubGrupos S'
      
        '  on (S.IdArea = A.IdArea And S.IdGrupo = A.IdGrupo and S.IdSubG' +
        'rupo = A.IdSubGrupo)'
      'Left Outer Join Colores K'
      '  on K.IdColor = A.IdColor'
      'Join Sucursales H'
      '  on H.IdSucursal = F.IdSucursal'
      'Join Clientes C'
      '  on C.IdCliente = F.Cliente And C.IdSucursal = F.IdSucursal'
      'Where (F.FechaF between :D and :H) and '
      '           (A.IdArea =:R) And (A.IdGrupo =:G)'
      'Order By H.DetSuc, S.SubGrupo, A.Marca')
    Left = 193
    Top = 296
    ParamData = <
      item
        DataType = ftDate
        Name = 'D'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'H'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'R'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'G'
        ParamType = ptUnknown
      end>
